        <div class="card-body">
          <div class="tab-content">
            <div class="tab-pane fade show active" id="Daily" role="tabpanel">
              <div class="d-flex flex-wrap align-items-center px-4 bg-light">
                <div class="mr-auto d-flex align-items-center py-3">
                  <span class="heart-ai bg-primary mr-3">
                    <i class="fa fa-heart-o" aria-hidden="true"></i>
                  </span>
                  <div>
                    <p class="fs-18 mb-2">Total Student that have passed this stage:</p>
                    <%
                    var x = 0;
                    for (var i = 0; i<students.length; i++){
                    if (students[i].Healthvr == true){
                    x = x + 1;
                    }
                  }
                %>
                    <span class="fs-26 text-primary font-w600"><%= x%>/<%= students.length %></span>
                  </div>
                </div>
                
              </div>

        <div class="row align-items-center">
          <div class="col-xl-6 col-xxl-12 col-md-6">
            <!-- <div id="radialBar"></div> -->
          </div>
          <div class="col-xl-6 col-xxl-12 col-md-6">
            <div class="d-flex mb-4 align-items-center">
              <span class="mr-auto pr-3 font-w500 fs-30 text-black">
                <svg class="mr-3" width="8" height="30" viewBox="0 0 8 30" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect width="7.65957" height="30" fill="#BDA25C"/>
                </svg>
                <%= (x/students.length) * 100%>
              </span>

        </div>



        <div class="table-responsive">
                        <table class="table table-hover">
                          <thead>
                            <tr>
                              <th>No.</th>
                              <th>StudID</th>
                              <th>First Name</th>
                              <th>Gender</th>
                              <th>Date of Birth</th>
                              <th>Nationality</th>
                              <th>House Address</th>
                              <th>Marital Status</th>
                              <th>Blood Group</th>
                              <th>Genotype</th>
                              <th>Medical Condition</th>
                              <th>Allergies</th>
                              <!-- <th>Date Registered</th> -->
                              <th>Guardian Name</th>
                              <th>Guardian Relationship</th>
                              <th>Guardian Address</th>
                              <th>Guardian Phone Number</th>      
                              <th>Healthvr</th>                     
                              <th>Action</th>
                              <!-- <th>Admin Verification</th> -->
                            </tr>
                          </thead>
                          <tbody>
                            <%for (var i = 0; i<students.length; i++){%>
                            <tr>
                                <td><%= i+1 %></td>
                                <td><%= students[i].StudID %></td>
                                <td><%= students[i].Firstname %></td>
                                <td><%= students[i].Gender %></td>
                                <td><%= students[i].DOB %></td>
                                <td><%= students[i].Nationality %></td>
                                <td><%= students[i].Address %></td>
                                <td><%= students[i].MaritalStatus %></td>
                                <td><%= students[i].BG %></td>
                                <td><%= students[i].GT %></td>
                                <td><%= students[i].MedicalCond %></td>
                                <td><%= students[i].Allergies %></td>
                                <td><%= students[i].Parentname %></td>
                                <td><%= students[i].ParentRel%></td>
                                <td><%= students[i].ParentAddress%></td>
                                <td><%= students[i].ParentPhone%></td>
                                <td><%= students[i].Healthvr%></td>  

                                <td>
                                  <a href="/update-oohealth?StudID=<%= students[i].StudID%>" class="btn border-shadow update" title="1">
                                    <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
                                  </a>
                                </td>
                            </tr>
                            <%}%>
                          </tbody>
