                        <%
                        const list = [];
                        var listc = 0;
                        for (var i = 0; i<students.length; i++){
                            if (students[i].Healthvr == true && students[i].Housingvr == true && students[i].Adminvr == true
                            && students[i].Financevr == true && students[i].Studactvr == true && students[i].Itvr == true ){
                            list[listc] =  i;
                            listc = listc + 1;
                            }
                        }
                        %>
  <br>
        <div class="table-responsive">
            <h2 class="text-black font-w600">COMPLETED STUDENTS</h2>
                        <table class="table table-hover">
                          <thead>
                            <tr>
                              <th>No.</th>
                              <th>ID</th>
                              <th>FName</th>
                              <th>LName</th>
                              <!-- <th>Completed Status</th> -->
                            </tr>
                          </thead>
                          <tbody>
                            <%for (var i = 0; i<list.length; i++){%>
                                <tr>
                                    <td class="<%= students[list[i]].StudID %>"><%= i+1 %></td>
                                    <td class="<%= students[list[i]].StudID %>"><%= students[list[i]].StudID %></td>
                                    <td class="<%= students[list[i]].StudID %>"><%= students[list[i]].Firstname %></td>
                                    <td class="<%= students[list[i]].StudID %>"><%= students[list[i]].Lastname %></td>
                                    <!-- <td>true</td> -->
                                </tr>
                                <%}%>
                          </tbody>
