                                        <label class="text-black font-w500">Payment Plan</label>
                                        <input type="checkbox">
                                        <span class="checkmark"></span>
                                      </div>
                                      </th>
                                      <!-- <br><br><br> -->
                                      <th>
                                      <label class="text-black font-w500">Finance Vr</label>
                                      <input type="checkbox">
                                      <span class="checkmark"></span>
                                      </th>
                                    </table>
                                    </thead>
                                    </tr>
                            
                          </div>

                          <div class="form-group">
                            <button type="button" class="btn btn-primary">CREATE REPORT</button>
                          </div>
                          </form>
