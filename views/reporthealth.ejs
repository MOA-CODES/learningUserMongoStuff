  <div class="container">

    <table class="table" id="example">
  <thead class="thead-dark">
    <tr>
      <th>Student Id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Gender</th>
      <th>Date of Birth</th>
      <th>Nationality</th>
      <th>House Address</th>
      <th>Email</th>
      <th>Mobile No</th>
      <th>Marital Status</th>
      <th>Blood Group</th>
      <th>Genotype</th>
      <th>Medical Condition</th>
      <th>Allergies</th>
      <th>Parent Name</th>
      <th>Parent Relationship</th>
      <th>Parent Address</th>
      <th>Parent Phone Number</th>

    </tr>
    <%for (var i = 0; i<students.length; i++){%>
      <tr>
        <td><%= students[i].StudID %></td>
        <td><%= students[i].Firstname %></td>
        <td><%= students[i].Lastname %></td>
        <td><%= students[i].Gender %></td>
        <td><%= students[i].DOB %></td>
        <td><%= students[i].Nationality %></td>
        <td><%= students[i].Address %></td>
        <td><%= students[i].SchoolEmail %></td>
        <td><%= students[i].Phonenumber %></td>
        <td><%= students[i].MaritalStatus %></td>
        <td><%= students[i].BG %></td>
        <td><%= students[i].GT %></td>
        <td><%= students[i].MedicalCond %></td>
        <td><%= students[i].Allergies %></td>
        <td><%= students[i].Parentname %></td>
        <td><%= students[i].ParentRel %></td>
        <td><%= students[i].ParentAddress %></td>
        <td><%= students[i].ParentPhone %></td>
      </tr>
      <%}%>
  </tbody>
</table>
<button id="csv" class="btn btn-info">Download Sheet</button>
  </div>
</div>
