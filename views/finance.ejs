  <section id="finanaceStage">
    <div class="container">
      <P> <br> <br> <br> </P>
        <h1 style="color: 103908; font-family: Lucida Fax; font-size: 30px;"  >STEP II: FINANCE</h1>
        <P> </P>
        <form action="" method="POST"  onsubmit="return false">
          <div class="form-group">
            <label class="now1" for="exampleInputEmail1">First Name:</label>
            <input type="text" value="<%- stud.Firstname%>" class="form-control" name="Firstname" id="firstt" aria-describedby="emailHelp" placeholder="Enter First Name"  style="background-color: FAF3F5;" required>
          </div>
          <P> </P>
          <div class="form-group">
              <label class="now1" for="exampleInputEmail1">Last Name:</label>
              <input type="text" value="<%- stud.Lastname%>" class="form-control" name="Lastname" id="lastt" aria-describedby="emailHelp" placeholder="Enter Last Name"  style="background-color: FAF3F5;"required>
          </div>
          <P> </P>
          <div class="form-group">
              <label class="now1" for="exampleInputEmail1">Middle Name:</label>
              <input type="text" value="<%- stud.Middlename%>" class="form-control" name="Middlename" id="midddle" aria-describedby="emailHelp" placeholder="Enter Middle Name"  style="background-color: FAF3F5;" required>
          </div>
          <P> </P>
          <div class="form-group">
              <label class="now1" for="exampleInputEmail1">Major:</label>
              <input type="text" value="<%- stud.Major%>" class="form-control" name="Major" id="fMajor" aria-describedby="emailHelp" placeholder="Enter Major"  style="background-color: FAF3F5;" required>
          </div>
          <P> </P>
          <div class="form-group">
              <label class="now1" for="exampleInputEmail1">Personal Email:</label>
              <input type="email" value="<%- stud.Personalemail%>" class="form-control" name="Personalemail" id="fEmail" aria-describedby="emailHelp" placeholder="Enter Email Address"  style="background-color: FAF3F5;" required>
          </div>
          <P> </P>
          <div class="form-group">
              <label class="now1" for="admission-status">Payment Plan:</label>
              <P> </P>
              <select style="background-color: FAF3F5;" name="PaymentPlan" id="admission-status" required>
                  <option value="DPP">Deffered Payment Plan</option>
                  <option value="Full">Full Payment</option>
                  <option value="Founder Scholarship">Founder Scholarship</option>
                  <option value="NFS">NFS Scholarship</option>
                  <option value="Shehu Atiku">Shehu Atiku</option>
                  <option value="Staff">Staff Plan</option>
              </select>
            </div>
            <p> </p>
            <div class="form-group">
              <label class="now1" for="exampleInputEmail1">Guardian Name:</label>
              <input type="text" class="form-control" name="GuardianName" id="gName" aria-describedby="emailHelp" placeholder="Enter Guardian Name"  style="background-color: FAF3F5;" required>
          </div>
          <P> </P>
          <div class="form-group">
              <label class="now1" class="now1"for="exampleInputEmail1">Guardian Relationship:</label>
              <input type="text" class="form-control" name="GuardianRel" id="gRelationship" aria-describedby="emailHelp" placeholder="Enter Relationship" style="background-color: FAF3F5;" required>
          </div>
            <p> </p>
            <P> </P>
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
          <div class="table-responsive">
              <table class="table table-hover">
                <thead>
                  <tr>
                    <th>ADMIN APPROVAL</th>
                    <th>STATUS</th>
                    <th>CHECK</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                      <td>OVERALL CHECK</td>
                      <td>APPROVED</td>
                      <td>
                      <label for="radio1"></label>
                      <input type="radio"  class="larger1" name="radios" id="finanaceOverall" />
                      </td>
                  </tr>
                </tbody>
              </table>
            </div>
            <a>
              <button class="previous" id="previousf" disabled>Previous</button>
            </a>
            <a href="/IT">
              <button class="next" id="nextf">Next</button>
            </a>
            <P> </P>
            <label for="accept">
              <input type="checkbox" id="accept1" onclick="myFunc()">
           </label>
  </section>
    </div>
    <script>
      var nextff = document.getElementById("nextf");
      nextff.disabled = true;
      var fradio = document.getElementById("finanaceOverall");
      function myFunc() {
      var checkBox = document.getElementById("accept1");
      if (checkBox.checked == true){
        fradio.checked = true;
        nextff.disabled = false;
      } else {
        fradio.checked = false;
        nextff.disabled = true;

      }
}
    </script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script type="text/javascript">
