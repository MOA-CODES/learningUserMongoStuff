						<h2 class="text-black font-w600">Dashboard</h2>
						<p class="mb-0">IT Admin Dashboard</p>
					</div>
				</div>


        <div class="card-body">
          <div class="tab-content">
            <div class="tab-pane fade show active" id="Daily" role="tabpanel">
              <div class="d-flex flex-wrap align-items-center px-4 bg-light">
                <div class="mr-auto d-flex align-items-center py-3">
                  <span class="heart-ai bg-primary mr-3">
                    <i class="fa fa-heart-o" aria-hidden="true"></i>
                  </span>
                  <div>
                    <p class="fs-18 mb-2">Total Student that have passed this stage:</p>
                    <%
                         var x = 0;
                         for (var i = 0; i<students.length; i++){
                         if (students[i].Itvr == true){
                         x = x + 1;
                        }
                      }
                    %>
                    <span class="fs-26 text-primary font-w600"><%= x%>/<%= students.length %></span>
                  </div>
                </div>
                
              </div>

        <div class="row align-items-center">
          <div class="col-xl-6 col-xxl-12 col-md-6">
            <!-- <div id="radialBar"></div> -->
          </div>
          <div class="col-xl-6 col-xxl-12 col-md-6">
            <div class="d-flex mb-4 align-items-center">
              <span class="mr-auto pr-3 font-w500 fs-30 text-black">
                <svg class="mr-3" width="8" height="30" viewBox="0 0 8 30" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect width="7.65957" height="30" fill="#BDA25C"/>
                </svg>
                <%= (x/students.length) * 100%>
              </span>

        </div>





        <div class="table-responsive">
                        <table class="table table-hover">
                          <thead>
                            <tr>
                              <th>No.</th>
                              <th>ID</th>
                              <th>First Name</th>
                              <th>Middle Name</th>
                              <th>Last Name</th>
                              <th>SchoolEmail</th>
                              <th>OpenerpStatus</th>
                              <th>Itvr</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <%for (var i = 0; i<students.length; i++){%>

                            <tr>
                                <td><%= i+1 %></td>
                                <td><%= students[i].StudID %></td>
                                <td><%= students[i].Firstname %></td>
                                <td><%= students[i].Middlename %></td>
                                <td><%= students[i].Lastname %></td>
                                <td><%= students[i].SchoolEmail %></td>
                                <td><%= students[i].OpenerpStatus %></td>
                                <td><%= students[i].Itvr %></td>
                                <td>
									<a href="/update-ooit?StudID=<%= students[i].StudID%>" class="btn border-shadow update" title="1">
										<span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
									</a>
								</td>
                            </tr>
                            <%}%>
                          </tbody>
                        </table>
                    </div>
                    <br><br><br>
                    <!-- <a href="javascript:void(0)" class="btn btn-outline-primary" data-toggle="modal" data-target="#addOrderModal">Generate Report</a> -->
                    
                  </div>
