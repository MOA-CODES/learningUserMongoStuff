                    <div class="modal-dialog" role="document">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title">SELECT WHICH THAT APPLIES</h5>
                          <button type="button" class="close" data-dismiss="modal"><span>&times;</span>
                          </button>
                        </div>
                        <div class="modal-body">
                          <form>
                            <table>
                              <thead>
                                <tr>
                                  <div class="form-group">
                                    <th>
              
                                      <label class="text-black font-w500">First Name</label>
                                      <input type="checkbox">
                                      <span class="checkmark"></span>
                                    </th>
                                    <!-- <br><br><br> -->
              
                                    <th>
                                      <div class="now" style="padding:15px" >
                                      <label class="text-black font-w500">Last Name</label>
                                      <input type="checkbox">
                                      <span class="checkmark"></span>
                                    </div>
                                    </th>
                                    <!-- <br><br><br> -->
                                    <th>
                                    <label class="text-black font-w500">Guardian Name</label>
                                    <input type="checkbox">
                                    <span class="checkmark"></span>
                                    </th>
              
                                    <!-- <br><br><br> -->
                            </table>
                            </thead>
                            </tr>
                            <table>
                              <thead>
                                <tr>
                                  <div class="form-group">
                                    <th>
              
                                      <label class="text-black font-w500">Guardian Relationship</label>
                                      <input type="checkbox">
                                      <span class="checkmark"></span>
                                    </th>
                                    <!-- <br><br><br> -->
              
                                    <th>
                                      <div class="now" style="padding:15px" >
                                      <label class="text-black font-w500">Payment Plan</label>
                                      <input type="checkbox">
                                      <span class="checkmark"></span>
                                    </div>
                                    </th>
                                    <!-- <br><br><br> -->
                                    <th>
                                    <label class="text-black font-w500">Finance Vr</label>
                                    <input type="checkbox">
                                    <span class="checkmark"></span>
                                    </th>
                                  </table>
                                  </thead>
                                  </tr>
                            <!-- <label class="text-black font-w500">HEALTH</label>
