        <div class="content-body">
			<div class="container-fluid">
				<div class="form-head d-flex align-items-center mb-sm-4 mb-3">
					<div class="mr-auto">
						<h2 class="text-black font-w600">Dashboard</h2>
						<p class="mb-0">Orientation Leader Admin Dashboard</p>
					</div>
				</div>


                <div class="card-body">
                    <div class="tab-content">
                      <div class="tab-pane fade show active" id="Daily" role="tabpanel">
                        <!-- <div class="d-flex flex-wrap align-items-center px-4 bg-light"> -->
                          <div class="mr-auto d-flex align-items-center py-3">
                            <!-- <span class="heart-ai bg-primary mr-3">
                              <i class="fa fa-heart-o" aria-hidden="true"></i>
                            </span> -->
                            <div class="col-lg-4">
                              <p class="fs-18 mb-2">Total Student that have passed <strong style="color: black;">Admission:</strong></p>
                              <span class="fs-26 text-primary font-w600">250/500</span>
                            </div>
                            <div class="col-lg-4">
                              <p class="fs-18 mb-2">Total Student that have passed <strong style="color: black;">Finance:</strong></p>
                              <span class="fs-26 text-primary font-w600">250/500</span>
                            </div>
                            <div class="col-lg-4">
                              <p class="fs-18 mb-2">Total Student that have passed <strong style="color: black;"> I.T:</strong></p>
                              <span class="fs-26 text-primary font-w600">250/500</span>
                            </div>
                          </div>
                        </div>
  
  
                  <div class="row align-items-center">
                    <!-- <div class="d-flex flex-wrap align-items-center px-4 bg-light"> -->
                      <div class="mr-auto d-flex align-items-center py-3">
                        <!-- <span class="heart-ai bg-primary mr-3">
                          <i class="fa fa-heart-o" aria-hidden="true"></i>
                        </span> -->
                        <div class="col-lg-4">
                          <p class="fs-18 mb-2">Total Student that have passed <strong style="color: black;">Housing/ID:</strong>:</p>
                          <span class="fs-26 text-primary font-w600">250/500</span>
                        </div>
                        <div class="col-lg-4">
                          <p class="fs-18 mb-2">Total Student that have passed <strong style="color: black;">Health:</strong></p>
                          <span class="fs-26 text-primary font-w600">250/500</span>
                        </div>
                        <div class="col-lg-4">
                          <p class="fs-18 mb-2">Total Student that have passed <strong style="color: black;">StudAct:</strong></p>
                          <span class="fs-26 text-primary font-w600">250/500</span>
                        </div>
                      </div>
  
                  </div>
  
  
  
  <br>
  <br>
  <br>


        <div class="table-responsive">
                        <table class="table table-hover">
                          <thead>
                            <tr>
                              <th>No.</th>
                              <th>ID</th>
                              <th>Name</th>
                              <th>Major</th>
                              <th>Email</th>
                              <th>SchoolEmail</th>
                              <th>Phone No</th>
                              <th>Admission</th>
                              <th>Finance</th>
                              <th>It</th>
                              <th>Housing & ID</th>
                              <th>Health</th>
                              <th>StudAct</th>
                            </tr>
                          </thead>
                          <tbody>
                            <%for (var i = 0; i<students.length; i++){%>
                            <tr>
                                <td><%= i+1 %></td>
                                <td><%= students[i].StudID %></td>
                                <td><%= students[i].Firstname %></td>
                                <td><%= students[i].Major %></td>
                                <td><%= students[i].Personalemail %></td>
                                <td><%= students[i].SchoolEmail %></td>

                                <td><%= students[i].Phonenumber %></td>
                                <td><%= students[i].Adminvr %></td>
                                <td><%= students[i].Financevr %></td>
                                <td><%= students[i].Itvr %></td>
                                <td><%= students[i].Housingvr %></td>
                                <td><%= students[i].Healthvr %></td>
                                <td><%= students[i].Studactvr %></td>                                
                            </tr>
                            <%}%>
                          </tbody>
                        </table>
                      </div>
                      <div class="">
                        <br>
                        <a href="" class="btn btn-outline-primary"><i class=""></i>Generate Report</a>
